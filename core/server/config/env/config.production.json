{
  "database": {
  "client": "postgres",
  "connection": {
      "host": "process.env.POSTGRES_HOST",
      "user": "process.env.POSTGRES_USER",
      "password": "process.env.POSTGRES_PASS",
      "database": "process.env.POSTGRES_DB",
      "port": "process.env.POSTGRES_PORT"
  }
},
    "auth": {
        "type": "password"
    },
    "paths": {
        "contentPath": "content/"
    },
    "logging": {
        "level": "info",
        "rotation": {
            "enabled": true
        },
        "transports": ["file", "stdout"]
    }
}
