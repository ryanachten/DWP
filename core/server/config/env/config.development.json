{
    "url": "https://dwp-ghost.herokuapp.com/",
    "fileStorage": false,
    "database": {
      "client": "pg",
      "connection": "process.env.DATABASE_URL",
      "debug": false
    },
    "auth": {
        "type": "password"
    },
    "paths": {
        "contentPath": "content/"
    },
    "privacy": {
        "useRpcPing": false,
        "useUpdateCheck": true
    },
    "useMinFiles": false,
    "caching": {
        "theme": {
            "maxAge": 0
        },
        "admin": {
            "maxAge": 0
        }
    }
}
